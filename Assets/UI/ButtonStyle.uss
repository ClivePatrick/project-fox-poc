.button-wrapper {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  align-self: flex-end;
  font-size: 16px;
  -unity-font-style: bold; 
}

.unity-button { 
  background-color: rgba(0, 0, 0, 0);
  color: white;
  width: 100%;
}

.level-button {
  border-width: 3px;
}

.level-image {
  margin: 10px;
}

.pause-button {
  border-width: 3px;
  border-color: white;
}